{
    "name": "salehhub/laravel-remote-logger",
    "description": "Send Laravel logs to a centralized remote logging server",
    "keywords": ["laravel", "logging", "remote", "centralized"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Saleh",
            "email": "saleh@example.com"
        }
    ],
    "require": {
        "php": "^8.1",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "illuminate/http": "^10.0|^11.0|^12.0",
        "illuminate/queue": "^10.0|^11.0|^12.0",
        "monolog/monolog": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "RemoteLogger\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "RemoteLogger\\RemoteLoggerServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
